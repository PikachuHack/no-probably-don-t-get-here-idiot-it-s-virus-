local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Pikzachu Dupe", "DarkTheme")

local Tab1 = Window:NewTab("Dupe")

local Tab1Section = Tab1:NewSection("dupe")

Tab1Section:NewToggle("piano dupe", "Toggles piano dupe On / Off.", function(state)

    if state then

        _G.on = true

        while _G.on do

            wait()

            -- Dupe Pop It Trading --

local args = {

    [1] = "Piano"

}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("Equip"):FireServer(unpack(args))

        end

    else

        _G.on = false

    end

end)
